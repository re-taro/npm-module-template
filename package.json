{
  "name": "npm-module-template",
  "version": "0.0.1",
  "description": "Template for personal npm module projects",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "module": "dist/index.es.js",
  "browser": "dist/library-name.js",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "types",
    "dist"
  ],
  "keywords": [],
  "author": {
    "name": "Rintaro Itokawa",
    "email": "me@re-taro.dev",
    "url": "https://www.re-taro.dev/"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "format": "eslint --fix src/**/*.ts",
    "prebuild": "rimraf dist",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/re-taro/npm-module-template"
  },
  "bugs": {
    "url": "https://github.com/re-taro/npm-module-template/issues"
  },
  "homepage": "https://github.com/re-taro/npm-module-template#readme",
  "dependencies": {
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@babel/preset-env": "7.16.11",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-commonjs": "21.0.2",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@rollup/plugin-typescript": "8.3.1",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "eslint": "8.10.0",
    "eslint-config-prettier": "8.5.0",
    "lodash.camelcase": "4.3.0",
    "lodash.upperfirst": "4.3.1",
    "prettier": "2.6.0",
    "rimraf": "3.0.2",
    "rollup": "2.70.0",
    "rollup-plugin-terser": "7.0.2",
    "typescript": "4.6.2"
  }
}
